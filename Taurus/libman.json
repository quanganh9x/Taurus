{
  "version": "1.0",
  "defaultProvider": "jsdelivr",
  "libraries": [
    {
      "library": "@aspnet/signalr@1.1.4",
      "destination": "wwwroot/lib/signalr/",
      "files": [
        "dist/browser/signalr.min.js"
      ]
    },
    {
      "library": "jquery@3.4.1",
      "destination": "wwwroot/lib/jquery/",
      "files": [
        "dist/jquery.min.js",
        "dist/jquery.min.map"
      ]
    },
    {
      "library": "bootstrap@4.3.1",
      "destination": "wwwroot/lib/bootstrap/",
      "files": [
        "dist/js/bootstrap.bundle.min.js",
        "dist/js/bootstrap.bundle.min.js.map"
      ]
    },
    {
      "provider": "unpkg",
      "library": "@fullcalendar/core@4.1.0",
      "destination": "wwwroot/lib/fullcalendar/",
      "files": [
        "main.min.js",
        "main.min.css"
      ]
    },
    {
      "provider": "unpkg",
      "library": "@fullcalendar/daygrid@4.1.0",
      "destination": "wwwroot/lib/fullcalendar/daygrid/",
      "files": [
        "main.min.css",
        "main.min.js"
      ]
    }
  ]
}